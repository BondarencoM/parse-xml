{
  "name": "@atscm/parse-xml",
  "version": "0.0.0",
  "description": "An XML parser that keeps track of the original document formatting",
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "out/types/index.d.ts",
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -cw",
    "format": "prettier --ignore-path .gitignore \"**/*\"",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "test": "nyc ava",
    "types": "tsc"
  },
  "keywords": [
    "xml",
    "parser",
    "parse",
    "format",
    "sax"
  ],
  "author": "Lukas Hechenberger <lukas.hechenberger@bachmann.info>",
  "license": "MIT",
  "devDependencies": {
    "@babel/code-frame": "^7.8.3",
    "@ls-age/eslint-config": "^0.9.2",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "ava": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  }
}
